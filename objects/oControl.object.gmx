pause_key = keyboard_check_pressed(ord("P"));
if (pause_key &amp;&amp; global.pause == 1) {
    global.pause = 0;
} else if (pause_key &amp;&amp; global.pause == 0) {
    global.pause = 1;
}
if (room == atk03) {
    var gameend = false;
    global.timecost++;
    if(!instance_exists(oHero)) {
        gameend = true;
        show_message("胜败乃兵家常事，请大侠重新来过");
    }
    if(timecost &gt;= room_speed &amp;&amp; !instance_exists(oEnemyKingofSlime) &amp;&amp; !instance_exists(oEnemyKSlime)) {
        var playername = get_string("你战胜了史莱姆之王，耗时："  
        + string(global.timecost/room_speed) + "秒,尊姓大名:", "");
        if (timecost &gt;= room_speed) {
            gameend = true;
            switch(global.rune) {
                case 1: global.runename = "焚";
                break;
                case 2: global.runename = "磐";
                break;
                case 3: global.runename = "怒";
                break;
            }
            http_get("http://ztory.cn/game/round/list.php?playername="+string(playername)+"&amp;rune="+global.runename+"&amp;timecost="+string(global.timecost/room_speed)+"&amp;version="+version);
        }
    }
    if(gameend &amp;&amp; timecost &gt; room_speed)
        if (show_question("是否查看排行榜？")) {
            url_open("http://ztory.cn/game/round/getlist.php");
            game_restart();
        } else {
            game_restart();
        }
}
