            <string>/// step
if (global.pause) exit;
if (!instance_exists(oHero)) exit; 
/// run current state
event_inherited();

if (hp &lt;= nexthp) {
    var two = 2;
    while (two) {
        var randomx  = random_range(140,430);
        var randomy  = random_range(70,240);
        while (!place_empty(randomx,randomy)) {
            randomx  = random_range(140,430);
            randomy  = random_range(70,240);
