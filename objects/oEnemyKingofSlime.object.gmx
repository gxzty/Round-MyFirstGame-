        var damage = instance_create(other.x+xdir, other.y+ydir, oDamage);
        damage.creator = id;
        if (state == scr_slime_dash_state) {
            other.isstun = true;
            other.stuntime = dashstuntime;
            damage.damage = dashdamage;
        } else {
            damage.damage = attack;
        }
        if (heavystrike) {
            other.isstun = true;
            other.stuntime = heavystrikestuntime;
            damage.damage = heavystrikedamage;
            heavystrike = false;
            alarm[4] = room_speed * heavystrikecd;
        }
        damage.knockback = knockback;
        state = scr_monster_stall_state;
        alarm[1] = room_speed;
    }
}
